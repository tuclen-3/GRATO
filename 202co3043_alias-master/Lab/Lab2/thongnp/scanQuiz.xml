
<!-- iPhone 11 Pro / X - 1 -->
<RelativeLayout
xmlns:android="http://schemas.android.com/apk/res/android"
android:id="@+id/iphone_11_p"
android:layout_width="fill_parent"
android:layout_height="fill_parent"
android:clipToOutline="true"
android:background="@drawable/iphone_11_p"
 />


<!-- drawable/iphone_11_p.xml -->
<vector
xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:aapt="http://schemas.android.com/aapt"
android:width="375dp"
android:height="668dp"
android:viewportWidth="375"
android:viewportHeight="668"
 >

<group>

<clip-path
android:pathData="M0 0H375V668H0V0Z"
/>

<path
android:pathData="M0 0V668H375V0"
android:fillColor="#FFFFFF"
/>

</group>

</vector>



<!-- Group -->
<View
android:id="@+id/group"
android:layout_width="0dp"
android:layout_height="0dp"
android:layout_alignParentLeft="true"
android:layout_alignParentRight="true"
android:layout_alignParentTop="true"
android:layout_alignParentBottom="true"
android:background="#00000000"
 />




<!-- Quiz Mar11 -->
<TextView
android:id="@+id/quiz_mar11"
android:layout_width="0dp"
android:layout_height="0dp"
android:layout_alignParentLeft="true"
android:layout_alignParentRight="true"
android:layout_alignParentTop="true"
android:layout_alignParentBottom="true"
android:text="@string/quiz_mar11"
android:textAppearance="@style/quiz_mar11"
android:gravity="top"
 />

<!--
  Font family: Sansation
  Line height: 20sp
-->
<!-- styles.xml -->
<style name="quiz_mar11">

<item name="android:textSize">
18sp
</item>

<item name="android:textColor">
#FFFFFF
</item>

</style>


<!-- strings.xml -->
<string name="quiz_mar11">

Quiz Mar11
</string>




